{
  "name": "qz-plugin",
  "version": "2.6.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elankiderman/qz-plugin.git"
  },
  "skpm": {
    "name": "qz-plugin",
    "manifest": "src/manifest.json",
    "main": "qz-plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  }
}
