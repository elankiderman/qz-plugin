{
  "name" : "QZ Plugin",
  "description":"internal tool for qz",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "invert",
      "shortcut" : "control shift i",
      "identifier": "qz-plugin.invert-identifier",
      "script": "./invert.js"
    },
    {
      "name": "toggle device",
      "shortcut" : "control shift d",
      "identifier": "qz-plugin.device-identifier",
      "script": "./device.js"
    },
    {
      "name": "toggle text size",
      "shortcut" : "control shift s",
      "identifier": "qz-plugin.type-identifier",
      "script": "./typeStyle.js"
    },
    {
      "name": "apply best fit text style",
      "shortcut" : "control shift n",
      "identifier": "qz-plugin.nearest-style-identifier",
      "script": "./nearestStyle.js"
    },
    {
      "name": "swap operating system",
      "shortcut" : "control shift o",
      "identifier": "qz-plugin.os-identifier",
      "script": "./swapOS.js"
    }
  ],
  "menu": {
    "title": "qz-plugin",
    "items": [
      "qz-plugin.invert-identifier",
      "qz-plugin.device-identifier",
      "qz-plugin.type-identifier",
      "qz-plugin.nearest-style-identifier",
      "qz-plugin.os-identifier"
    ]
  }
}
